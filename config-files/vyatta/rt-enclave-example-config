firewall {
    all-ping enable
    broadcast-ping disable
    group {
        port-group ALLOWED_DST_PORTS {
            port 443
            port 943
            port 8443
            port 9443
            port 53
            port 80
            port 67
            port 68
            port 9022
            port 123
        }
        port-group ALLOWED_PORTS {
            port 443
            port 943
            port 53
            port 80
            port 8443
            port 9022
            port 67
            port 68
        }
        port-group ALLOWED_SRC_PORTS {
            port 67
            port 68
            port 53
            port 123
        }
    }
    ipv6-receive-redirects disable
    ipv6-src-route disable
    ip-src-route disable
    log-martians enable
    name FROM_TRUSTED_TO_LOCAL {
        default-action accept
        rule 10 {
            action accept
            description "Traffic from Trusted Zone to Local Zone"
        }
    }
    name FROM_TRUSTED_TO_UNTRUSTED {
        default-action accept
        enable-default-log
        rule 10 {
            action accept
            description "Traffic from Trusted Zone to Untrusted Zone"
        }
    }
    name FROM_UNTRUSTED_TO_LOCAL {
        default-action drop
        rule 10 {
            action accept
            description "Traffic from Untrusted Zone to Local Zone"
            destination {
                group {
                    port-group ALLOWED_DST_PORTS
                }
            }
        }
        rule 11 {
            action accept
            source {
                group {
                    port-group ALLOWED_SRC_PORTS
                }
            }
        }
        rule 101 {
            action drop
            description "Drop and log all packets hitting this rule"
            log enable
        }
    }
    name FROM_UNTRUSTED_TO_TRUSTED {
        default-action reject
        description "Traffic from Untrusted Zone to Trusted Zone"
        enable-default-log
        rule 10 {
            action accept
            destination {
                group {
                    port-group ALLOWED_DST_PORTS
                }
            }
            protocol tcp_udp
        }
        rule 11 {
            action accept
            protocol tcp_udp
            source {
                group {
                    port-group ALLOWED_SRC_PORTS
                }
            }
        }
        rule 12 {
            action accept
            protocol icmp
        }
        rule 101 {
            action drop
            description "Drop and log all packets"
            log enable
        }
    }
    receive-redirects enable
    send-redirects enable
    source-validation disable
    state-policy {
        established {
            action accept
        }
        invalid {
            action accept
            log {
                enable
            }
        }
        related {
            action accept
        }
    }
    syn-cookies enable
}
