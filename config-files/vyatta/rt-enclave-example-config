    zone LOCAL {
        default-action drop
        from TRUSTED {
            firewall {
                name FROM_TRUSTED_TO_LOCAL
            }
        }
        from UNTRUSTED {
            firewall {
                name FROM_UNTRUSTED_TO_LOCAL
            }
        }
        local-zone
    }
    zone TRUSTED {
        default-action reject
        description "Limited access VLAN 16: Internal Interface of Enclave VPN"
        from UNTRUSTED {
